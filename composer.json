{
    "name": "shinjin/freezer",
    "description": "A cool object storage library",
    "keywords": ["freezer", "object storage"],
    "homepage": "http://github.com/shinjin/freezer",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rick Shin"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "doctrine/cache": "^1.0",
        "shinjin/pdo": "^0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Freezer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Freezer\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}